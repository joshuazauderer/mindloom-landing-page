{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api|videos|logo\\.png|styles\\.css|.*\\.(css|js|png|jpg|jpeg|gif|svg|mp4|woff|woff2|ttf|eot|otf|wasm|json|ico)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}

